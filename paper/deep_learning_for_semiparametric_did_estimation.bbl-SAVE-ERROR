    \entry{goodman-baconDifferenceindifferencesVariationTreatment2021}{article}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=437b3fad5b4a40d10f45b4fc16dd7730}{%
           family={Goodman-Bacon},
           familyi={G\bibinithyphendelim B\bibinitperiod},
