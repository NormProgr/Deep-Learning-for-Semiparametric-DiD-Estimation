      \field{title}{Difference-in-Differences with Variation in Treatment Timing}
      \field{urlday}{18}
      \field{urlmonth}{6}
      \field{urlyear}{2024}
      \field{volume}{225}
      \field{year}{2021}
      \field{dateera}{ce}
      \field{urldateera}{ce}
      \field{pages}{254\bibrangedash 277}
      \range{pages}{24}
