      \strng{namehash}{6c47f1fd7a83092f82c2766ab840a44b}
      \strng{fullhash}{6c47f1fd7a83092f82c2766ab840a44b}
      \strng{bibnamehash}{6c47f1fd7a83092f82c2766ab840a44b}
      \strng{authorbibnamehash}{6c47f1fd7a83092f82c2766ab840a44b}
      \strng{authornamehash}{6c47f1fd7a83092f82c2766ab840a44b}
      \strng{authorfullhash}{6c47f1fd7a83092f82c2766ab840a44b}
